n  content: \"\";\n  content: none;\n}\ntable {\n  border-collapse: collapse;\n  border-spacing: 0;\n}\n\n// common\n// variables\n\n// Ширины\n\n$screen-xs: 0;\n$screen-sm: 480px;\n$screen-md: 768px;\n$screen-lg: 992px;\n$screen-xl: 1200px;\n$screen-xxl: 1800px;\n\n$main-color: #7290a1;\n$accent-color: #0077ff;\n$main-bg-color: #ffffff;\n\n// styles\nhtml {\n  font-family: Gilroy;\n  font-size: 16px;\n  line-height: 140%;\n  color: $main-color;\n}\n\nbody {\n  min-width: 320px;\n}\n\n.page {\n  .page__header {\n    height: 64px;\n\n    @media screen and (max-width: $screen-xl) {\n      height: 56px;\n    }\n  }\n}\n\n.header {\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  background: $main-bg-color;\n  box-shadow: 0px 4px 28px rgba(65, 109, 131, 0.05);\n\n  @media screen and (max-width: $screen-xl) {\n    justify-content: flex-start;\n    padding: 0 40px;\n    height: 56px;\n  }\n\n  @media screen and (max-width: $screen-sm) {\n    padding: 0 16px;\n  }\n\n  &__burger {\n    display: none;\n    cursor: pointer;\n\n    @media screen and (max-width: $screen-xl) {\n      display: block;\n    }\n  }\n\n  &__logo {\n    cursor: pointer;\n\n    @media screen and (max-width: $screen-xl) {\n      margin-left: 24px;\n\n      svg {\n        width: 86px;\n        height: 20px;\n      }\n    }\n\n    @media screen and (max-width: $screen-sm) {\n      margin-left: 16px;\n    }\n  }\n\n  &__nav {\n    @media screen and (max-width: $screen-xl) {\n      display: block;\n      position: absolute;\n      top: 58px;\n      left: 0;\n      background: $main-bg-color;\n      box-shadow: 0px 4px 28px rgba(65, 109, 131, 0.05);\n    }\n  }\n\n  &__download-link {\n    margin-left: 40px;\n    cursor: pointer;\n    color: $accent-color;\n\n    @media screen and (max-width: $screen-xl) {\n      margin-left: auto;\n    }\n\n    @media screen and (max-width: $screen-sm) {\n      font-size: 14/16 * 1rem;\n    }\n  }\n}\n\nnav {\n  margin-left: 108px;\n\n  @media screen and (max-width: $screen-xl) {\n    margin-left: 0;\n    display: flex;\n    flex-direction: column;\n    min-width: 255px;\n  }\n  @media screen and (max-width: $screen-sm) {\n    padding-bottom: 16px;\n  }\n\n  &__item {\n    margin-left: 40px;\n    cursor: pointer;\n\n    @media screen and (max-width: $screen-xl) {\n      margin-bottom: 24px;\n    }\n\n    @media screen and (max-width: $screen-sm) {\n      margin-left: 16px;\n    }\n\n    &:first-child {\n      margin-left: 0;\n\n      @media screen and (max-width: $screen-xl) {\n        margin-left: 40px;\n      }\n\n      @media screen and (max-width: $screen-sm) {\n        margin-left: 16px;\n      }\n    }\n  }\n}\n\n.hidden {\n  display: none;\n}\n\n.section1 {\n  background-image: url(\"./images/loon-image-original.jpg\");\n  width: 100%;\n  min-height: calc(100vh - 64px);\n  background-size: cover;\n  background-position-x: center;\n\n  @media screen and (max-width: $screen-xl) and (min-width: $screen-sm + 1px) {\n    background-image: url(\"./images/main-img-tablet.jpg\");\n    background-position-x: left;\n  }\n\n  @media screen and (max-width: $screen-sm) and (min-width: $screen-xs) {\n    background-image: url(\"./images/loon-image-original-min.jpg\");\n    background-position-x: left;\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"c08a3da79cac86df467ac830c19915fd","cacheData":{"env":{}}}